<?xml version="1.0" encoding="utf-8"?>
<example.getlearn.tv.util.LinearLayoutUtils
    android:padding="5dp"
    android:layout_marginLeft="@dimen/sw_10dp"
    android:id="@+id/adapter_vip_tem_ll_utils"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_marginTop="@dimen/sw_65dp"
    android:layout_width="wrap_content"
    android:layout_height="@dimen/sw_225dp">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:scaleType="fitXY"
            android:id="@+id/adapter_vip_tem_imag"
            android:layout_width="@dimen/sw_120dp"
            android:layout_height="@dimen/sw_180dp" />

        <TextView
            android:id="@+id/adapter_vip_item_txt"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_alignParentBottom="true"
            android:textSize="@dimen/sw_20dp"
            android:textStyle="bold"
            android:textColor="@color/color_17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <!--<TextView-->
            <!--android:id="@+id/adapter_vip_item_txt"-->
            <!--android:gravity="center_horizontal"-->
            <!--android:textSize="@dimen/sw_20dp"-->
            <!--android:textStyle="bold"-->
            <!--android:textColor="@color/color_17"-->
            <!--android:paddingBottom="@dimen/sw_75dp"-->
            <!--android:layout_alignParentBottom="true"-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="wrap_content" />-->

        <TextView
            android:id="@+id/adapter_vip_item_text_ordered"
            android:layout_gravity="center"
            android:gravity="center"
            android:visibility="gone"
            android:layout_marginTop="@dimen/sw_225dp"
            android:textColor="@color/color_17"
            android:textSize="@dimen/sw_18dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/adapter_vip_item_text_validTime"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_marginTop="@dimen/sw_245dp"
            android:visibility="gone"
            android:textSize="@dimen/sw_16dp"
            android:textColor="@color/color_17"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

    </RelativeLayout>

</example.getlearn.tv.util.LinearLayoutUtils>